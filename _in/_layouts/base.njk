      <nav>
        <ol>
          <li>
            <a href="/demos" {% if 'demos' in page.filePathStem %}aria-current="page"{% endif %}>Demos</a>
          </li>
          <li>
            <a href="/docs" {% if 'docs' in page.filePathStem %}aria-current="page"{% endif %}>Docs</a>
          </li>
          {# <li>
            <a href="/about" {% if 'about' in page.filePathStem %}aria-current="page"{% endif %}>About</a>
          </li> #}
          {# <li>
            <a href="/patterns" {% if 'patterns' in page.filePathStem %}aria-current="page"{% endif %}>Patterns</a>
          </li> #}
        </ol>
      </nav>   
    </header>
    
    <main id="content">
      <section>
        {% if subtitle %}
          <hgroup>
            <h1>{{ title }}</h1>
            <p>{{ subtitle }}</p>
          </hgroup>
        {% else %}
          <h1>{{ title }}</h1>
        {% endif %}

        {{ intro | safe }}
      </section>
      
      {{ content | safe }}
    </main>

    <footer id="footer">
      built by <a href="https://matuzo.at">matuzo</a> in Graz
    </footer>
  </body>
</html>